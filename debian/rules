#!/usr/bin/make -f

# Uncomment this to turn on verbose mode.
#export DH_VERBOSE=1

%:
	dh $@

override_dh_auto_configure:
	cp src/config/app.prod.js src/config/app.js

override_dh_auto_install:
	npm install -g npm@6.14.4
	npm ci
	npm run build

override_dh_auto_clean:
	rm -f src/config/app.js
	rm -rf dist
