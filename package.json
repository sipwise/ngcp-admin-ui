{
  "name": "ngcp-admin-ui",
  "version": "0.0.1",
  "description": "User interface to administer the NGCP platform",
  "productName": "NGCP Admin UI",
  "cordovaId": "com.sipwise.ui.admin",
  "capacitorId": "",
  "author": "Hans-Peter Herzog <hherzog@sipwise.com>",
  "private": true,
  "scripts": {
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore ./",
    "test": "echo \"No test specified\" && exit 0",
    "build": "quasar build",
    "dev": "quasar dev",
    "upgrade": "quasar upgrade",
    "upgrade:conduct": "quasar upgrade -i",
    "docker:kill": "./bin/docker-kill.sh",
    "docker:pull": "docker pull docker.mgm.sipwise.com/ngcp-admin-ui-buster:latest",
    "docker:run": "docker run --name=ngcp-admin-ui --rm -p 8080:8080 -i -t -v $(pwd):/code:rw docker.mgm.sipwise.com/ngcp-admin-ui-buster:latest t/run_admin_ui $1",
    "dev:docker": "yarnpkg run docker:kill && yarnpkg run docker:pull && yarnpkg run docker:run",
    "new:store": "quasar new store $1",
    "new:page": "quasar new page $1",
    "i18n:import": "node ./bin/i18n-import.js",
    "i18n:extract": "sh ./bin/vue-i18n-extract/extract.sh",
    "i18n:extract-report": "sh ./bin/vue-i18n-extract/extract.sh report"
  },
  "dependencies": {
    "@quasar/extras": "^1.9.10",
    "axios": "^0.21.1",
    "quasar": "^1.14.3",
    "validator": "13.5.2",
    "vue-i18n": "^8.0.0"
  },
  "devDependencies": {
    "@quasar/app": "^1.8.10",
    "@quasar/cli": "^1.1.2",
    "babel-eslint": "^10.0.1",
    "commander": "6.2.1",
    "dot-object": "2.1.4",
    "eslint": "^6.8.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-vue": "^6.1.2",
    "file-saver": "^2.0.2",
    "glob": "7.1.6",
    "is-valid-glob": "1.0.0",
    "js-yaml": "3.14.1",
    "json-beautify": "1.1.1",
    "jwt-decode": "2.2.0",
    "lodash": "^4.17.15",
    "po2json": "0.4.5",
    "qs": "^6.9.3",
    "vue-password-strength-meter": "^1.7.2",
    "vue-wait": "1.4.8",
    "vuelidate": "^0.7.5",
    "zxcvbn": "^4.4.2"
  },
  "engines": {
    "node": ">= 10.18.1",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  },
  "browserslist": [
    "last 1 version, not dead, ie >= 11"
  ],
  "resolutions": {
    "@babel/parser": "7.7.5"
  }
}
